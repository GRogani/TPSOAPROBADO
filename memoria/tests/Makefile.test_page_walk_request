# Makefile for test_page_walk_request

# Compiler and flags
CC = gcc
CFLAGS = -Wall -Werror -g -std=c11

# Target
TARGET = test_page_walk_request

# Object files
OBJS = test_page_walk_request.o

# Default rule
all: $(TARGET)

# Linking rule
$(TARGET): $(OBJS)
	$(CC) $(CFLAGS) -o $@ $^

# Compilation rule
%.o: %.c
	$(CC) $(CFLAGS) -c $< -o $@

# Clean rule
clean:
	rm -f $(TARGET) $(OBJS)

# Run rule
run: $(TARGET)
	./$(TARGET)

# Default rule
.PHONY: all clean run
